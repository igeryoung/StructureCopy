{
  "name": "structure-copy",
  "displayName": "Structure Copy",
  "description": "copy file and folder with thier structure for GPT input",
  "version": "0.0.1",
  "publisher": "shengping-yang",
  "repository": {
    "type": "git",
    "url": "https://github.com/shengping-yang/structure-copy.git"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
          "command": "copyByExtension.copy",
          "title": "Structure Copy"
        },
        {
          "command": "copyByExtension.copyPlain",
          "title": "Structure Copy Folder"
        },
        {
          "command": "copyByExtension.copyRecursive",
          "title": "Structure Copy Folder Recursive"
        }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder == false",
           "command": "copyByExtension.copy",
           "group": "2_modification"
        },
         {
           "when": "explorerResourceIsFolder == true",
           "command": "copyByExtension.copyPlain",
           "group": "2_modification"
         },
         {
           "when": "explorerResourceIsFolder == true",
           "command": "copyByExtension.copyRecursive",
           "group": "2_modification"
         }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
  
}
